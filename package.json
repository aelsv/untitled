    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-redux": "^7.0.3",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-helmet": "^5.2.1"
  }
}
